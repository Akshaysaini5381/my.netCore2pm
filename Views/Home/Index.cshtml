@model List<my.netCore2pm.Models.A_Model>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="addfrom">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
              Name
            </th>
            <th>
                Email
            </th>
            <th>
              City
            </th>
            <th>
               College
            </th>
            <th>Edit/Delete</th>
     
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
              @item.Name
            </td>
            <td>
               @item.Email
            <td>
              @item.City
            </td>
            <td>
               @item.College
            </td>
            <td>
                <a class="btn btn-success" asp-action="edit" asp-route-id="@item.Id">Edit</a> |
               
                <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
